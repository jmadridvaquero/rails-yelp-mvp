<%= simple_form_for [@restaurant] do |f| %>
  <%= f.input :name %>

   <%= f.input :address %>
  
   <%= f.input :phone_number %>


   <%= f.input :category %>
  <%= f.submit "add a restaurant", method: :post, data: { confirm: "You sure of this? you won't be able to remove it later without an admin" }, class: "btn btn-primary" %>
<% end %>

